{
  "name": "git-graft",
  "description": "git hook generator that prepends branch name to commit message",
  "version": "0.0.1-alpha",
  "author": "JacobMGEvans @JacobMGEvans",
  "bin": {
    "git-graft": "./bin/run"
  },
  "bugs": "https://github.com/JacobMGEvans/git-graft/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "conf": "^10.0.1",
    "enquirer": "^2.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "execa": "^5.0.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.17",
    "@types/fs-extra": "^9.0.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^15.0.2",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/JacobMGEvans/git-graft",
  "keywords": [
    "oclif"
  ],
  "license": "GPLv3",
  "main": "lib/index.js",
  "oclif": {
    "bin": "git-graft"
  },
  "repository": "JacobMGEvans/git-graft",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && mkdir ./lib/templates/ && cp ./src/templates/git-graft-template ./lib/templates/ && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
