{
  "name": "git-graft",
  "description": "git hook generator that prepends branch name to commit message",
  "version": "0.0.17-alpha",
  "author": "JacobMGEvans",
  "bin": {
    "git-graft": "./bin/run"
  },
  "bugs": "https://github.com/JacobMGEvans/git-graft/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "chalk": "^4.1.1",
    "enquirer": "^2.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.3",
    "@types/update-notifier": "^5.0.0",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/JacobMGEvans/git-graft",
  "keywords": [
    "oclif",
    "git",
    "git-hook",
    "npx",
    "commit",
    "commit-msg"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "git-graft"
  },
  "repository": "JacobMGEvans/git-graft",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && mkdir ./lib/templates/ && cp ./src/templates/git-graft-template ./lib/templates/ && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
